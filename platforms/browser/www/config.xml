<?xml version='1.0' encoding='utf-8'?>
<widget id="agency.serendip.phonegap_fcm_push_example" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>
		FCM Push
	</name>
    <description>
		This project is an example of implementing fcm push with phonegap.
	</description>
    <author email="info@serendip.agency" href="https://serendip.agency">
		Serendip Agency
	</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="phonegap-version" value="cli-7.0.1" />
</widget>
